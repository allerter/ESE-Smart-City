  <Requirements:Requirement xmi:id="_9VHFYJIGEe60IZayPKr4aA" base_NamedElement="_9VECEJIGEe60IZayPKr4aA" id="1" text="The system shall facilitate an infrastructure to provide a reliable communication between the vehicles and the system, enabling autonomous cars to share information about their current location, destination &amp; road conditions. " base_Class="_9VECEJIGEe60IZayPKr4aA"/>
  <Requirements:Requirement xmi:id="_M8cmoJIHEe60IZayPKr4aA" base_NamedElement="_M8YVMJIHEe60IZayPKr4aA" id="2" text="The city will already have an appropriated infrastructure to facilitate the system, such as: &#xD;&#xA;Traffic lights, Sensors, such as: Cameras, Light sensors, Etcâ€¦, Infrastructure for wireless communication" base_Class="_M8YVMJIHEe60IZayPKr4aA"/>
  <Requirements:Requirement xmi:id="_ZvYtoJIHEe60IZayPKr4aA" base_NamedElement="_ZvWRYJIHEe60IZayPKr4aA" id="3" base_Class="_ZvWRYJIHEe60IZayPKr4aA"/>
  <Requirements:Requirement xmi:id="_djsFsJIHEe60IZayPKr4aA" base_NamedElement="_djq3kJIHEe60IZayPKr4aA" id="4" base_Class="_djq3kJIHEe60IZayPKr4aA"/>
  <Requirements:Requirement xmi:id="_kaN4oJIHEe60IZayPKr4aA" base_NamedElement="_kaNRkJIHEe60IZayPKr4aA" id="5" text="The system shall have security measures to protect itself from unauthorized access &amp; data breaches." base_Class="_kaNRkJIHEe60IZayPKr4aA"/>
  <Requirements:Requirement xmi:id="_nSDBYJIHEe60IZayPKr4aA" base_NamedElement="_nR_XAJIHEe60IZayPKr4aA" id="6" text="The system shall comply with the local data privacy compliance laws. " base_Class="_nR_XAJIHEe60IZayPKr4aA"/>
  <Requirements:Requirement xmi:id="_sG860JIHEe60IZayPKr4aA" base_NamedElement="_sG7FoJIHEe60IZayPKr4aA" id="7" text="The system shall be able to detect faults within itself and react accordingly. " base_Class="_sG7FoJIHEe60IZayPKr4aA"/>
  <Requirements:Requirement xmi:id="_6CMGoJIHEe60IZayPKr4aA" base_NamedElement="_6CGAAJIHEe60IZayPKr4aA" id="8" text="The system shall reduce the city's carbon footprint" base_Class="_6CGAAJIHEe60IZayPKr4aA"/>
  <Requirements:Requirement xmi:id="__sLN8JIHEe60IZayPKr4aA" base_NamedElement="__sIxsJIHEe60IZayPKr4aA" id="9" text="The system should comply with the green initives set by the city, region, and the government." base_Class="__sIxsJIHEe60IZayPKr4aA"/>
  <Requirements:Requirement xmi:id="_HTY28JIIEe60IZayPKr4aA" base_NamedElement="_HTVzoJIIEe60IZayPKr4aA" id="10" base_Class="_HTVzoJIIEe60IZayPKr4aA"/>
  <Requirements:Requirement xmi:id="_0U2MgJILEe60IZayPKr4aA" base_NamedElement="_0UzwQJILEe60IZayPKr4aA" id="11" text="The system should conserve energy where possible" base_Class="_0UzwQJILEe60IZayPKr4aA"/>
  <Requirements:Requirement xmi:id="_STsl4JIMEe60IZayPKr4aA" base_NamedElement="_STqwsJIMEe60IZayPKr4aA" id="12" base_Class="_STqwsJIMEe60IZayPKr4aA"/>
  <Requirements:Requirement xmi:id="_hoF9AJIMEe60IZayPKr4aA" base_NamedElement="_hoEH0JIMEe60IZayPKr4aA" id="13" text="The system shall provide means of real-time feedback from citizens for notifying issues such as potholes, traffick problems, etc." base_Class="_hoEH0JIMEe60IZayPKr4aA"/>
